package com.isobar.isohealth.models;

public class NewWeightMeasurement {
	String timestamp; // The time at which the measurement was taken (e.g., "Sat, 1 Jan 2011 00:00:00")
	
	// One of the following values: weight, free_mass, mass_weight, fat_percent, bmi
	Double weight; // The value of the measured quantity
	Double free_mass; // The value of the measured quantity
	Double mass_weight; // The value of the measured quantity
	Double fat_percent; // The value of the measured quantity
	Double bmi; // The value of the measured quantity
	
	Boolean post_to_twitter; // Whether to post this measurement to Twitter (optional; if omitted, the user's default setting will be used)
	Boolean post_to_facebook; // Whether to post this measurement to Facebook (optional; if omitted, the user's default setting will be used)
}
